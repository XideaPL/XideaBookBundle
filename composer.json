{
    "name": "xideapl/book-bundle",
    "type": "symfony-bundle",
    "description": "Symfony XideaBookBundle",
    "keywords": [],
    "homepage": "http://xidea.pl",
    "authors": [
        {
            "name": "Artur Pszczółka",
            "email": "artur.pszczolka@xidea.pl"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "symfony/symfony": "2.6.*",
        "twig/twig": "~1.12",
        "xideapl/book": "dev-master@dev"
    },
    "require-dev": {
        "doctrine/orm": "*",
        "doctrine/doctrine-bundle": "*",
        "doctrine/doctrine-fixtures-bundle": "dev-master",
        "doctrine/data-fixtures": "dev-master",
        "doctrine/doctrine-migrations-bundle": "dev-master",
        "doctrine/migrations": "dev-master",
        "xideapl/user-bundle": "dev-master@dev"
    },
    "autoload": {
        "psr-0": { "Xidea\\Bundle\\BookBundle": "" }
    },
    "target-dir": "Xidea/Bundle/BookBundle",
	"extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}